{% comment %} <h1>{{ 'general.404.title' | t }}</h1> {% endcomment %}
{% comment %} <p>{{ 'general.404.subtext_html' | t }}</p> {% endcomment %}
<h1>{{ section.settings.title }}</h1>
<h1>{{ section.settings.subtext_title }}</h1>

{% schema %}
    {
        "name": "Template 404",
        "settings": [
            {
                "type": "text",
                "id": "title",
                "label": "404 Heading Title",
                "default": "404"
            },
            {
                "type": "text",
                "id": "subtext_title",
                "label": "404 Subtext Title",
                "default": "The page that you're looking for is not found."
            }
        ]
    }
{% endschema %}